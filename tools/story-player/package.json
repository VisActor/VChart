{
  "name": "@internal/story-player",
  "version": "0.0.3",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "types": "esm/index.d.ts",
  "files": [
    "esm",
    "cjs",
    "build"
  ],
  "scripts": {
    "dev": "vite",
    "build": "bundle --clean",
    "preview": "vite preview",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check .",
    "lint": "eslint --debug --fix src/",
    "eslint": "eslint --debug --fix src/",
    "eslint:check": "eslint src/",
    "eslint:err": "eslint src/ --quiet",
    "build:es": "bundle --clean -f es --ignorePostTasks"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@internal/bundler": "workspace:*",
    "@internal/eslint-config": "workspace:*",
    "@internal/ts-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "@types/jest": "^26.0.0",
    "@types/node": "*",
    "@typescript-eslint/eslint-plugin": "5.30.0",
    "@typescript-eslint/parser": "5.30.0",
    "cross-env": "^7.0.3",
    "eslint": "~8.18.0",
    "jest": "^26.0.0",
    "prettier": "2.6.1",
    "ts-jest": "^26.0.0",
    "ts-node": "10.9.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "4.9.5",
    "vite": "3.2.6"
  },
  "dependencies": {
    "@visactor/vchart": "workspace:1.3.1",
    "@visactor/vrender": "0.15.0-alpha.17",
    "@visactor/vutils": "~0.15.13-alpha.3"
  }
}
