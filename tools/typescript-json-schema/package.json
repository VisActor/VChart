{
  "name": "@internal/typescript-json-schema",
  "version": "0.0.1",
  "private": true,
  "description": "typescript-json-schema generates JSON Schema files from your Typescript sources",
  "main": "dist/typescript-json-schema.js",
  "typings": "dist/typescript-json-schema.d.ts",
  "bin": {
    "schema": "./bin/typescript-json-schema"
  },
  "dependencies": {
    "@types/json-schema": "^7.0.9",
    "@types/node": "*",
    "glob": "^7.1.7",
    "path-equal": "^1.2.5",
    "safe-stable-stringify": "^2.2.0",
    "ts-node": "10.9.0",
    "typescript": "4.9.5",
    "yargs": "^17.1.1"
  },
  "devDependencies": {
    "@types/chai": "^4.2.21",
    "@types/glob": "^7.1.4",
    "@types/mocha": "^9.0.0",
    "ajv": "^8.12.0",
    "ajv-formats": "^2.1.1",
    "chai": "^4.3.4",
    "mocha": "^9.1.3",
    "source-map-support": "^0.5.20"
  },
  "scripts": {
    "postinstall": "npm run build",
    "prepublishOnly": "npm run build",
    "debug": "node --inspect=19248 --inspect-brk -r ts-node/register typescript-json-schema-cli.ts",
    "run": "rm -rf ./dist && ts-node typescript-json-schema-cli.ts",
    "build": "tsc",
    "dev": "tsc -w",
    "test:dev": "mocha -t 5000 --watch --require source-map-support/register dist/test"
  }
}
