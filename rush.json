{
  "$schema": "https://developer.microsoft.com/json-schemas/rush/v5/rush.schema.json",
  "rushVersion": "5.94.1",
  "pnpmVersion": "7.32.1",
  "nodeSupportedVersionRange": ">=14.15.0 <15.0.0 || >=16.13.0 <17.0.0 || >=18.15.0 <19.0.0",
  "suppressNodeLtsWarning": true,
  "ensureConsistentVersions": true,
  "projectFolderMinDepth": 2,
  "projectFolderMaxDepth": 2,
  "allowMostlyStandardPackageNames": false,
  "gitPolicy": {
    "versionBumpCommitMessage": "Bump versions [skip ci]",
    "changeLogUpdateCommitMessage": "Update changelogs [skip ci]"
  },
  "repository": {
    "url": "https://github.com/VisActor/VChart",
    "defaultBranch": "main",
    "defaultRemote": "origin"
  },
  "allowedProjectTags": ["tool", "share", "package"],
  "projects": [
    {
      "packageName": "@visactor/vchart",
      "projectFolder": "packages/vchart",
      "tags": ["package"],
      "shouldPublish": true
    },
    {
      "packageName": "@visactor/react-vchart",
      "projectFolder": "packages/react-vchart",
      "tags": ["package"],
      "shouldPublish": true
    },
    {
      "packageName": "@visactor/taro-vchart",
      "projectFolder": "packages/taro-vchart",
      "tags": ["package"],
      "shouldPublish": true
    },
    {
      "packageName": "@visactor/tt-vchart",
      "projectFolder": "packages/tt-vchart",
      "shouldPublish": false
    },
    {
      "packageName": "@visactor/lark-vchart",
      "projectFolder": "packages/lark-vchart",
      "shouldPublish": false
    },
    {
      "packageName": "@internal/bundler",
      "projectFolder": "tools/bundler",
      "tags": ["tool"],
      "shouldPublish": false
    },
    {
      "packageName": "@internal/eslint-config",
      "projectFolder": "share/eslint-config",
      "tags": ["share"],
      "shouldPublish": false
    },
    {
      "packageName": "@internal/ts-config",
      "projectFolder": "share/ts-config",
      "tags": ["share"],
      "shouldPublish": false
    }
  ]
}
