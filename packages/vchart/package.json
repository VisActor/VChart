{
  "name": "@visactor/vchart",
  "version": "1.1.0-beta.7",
  "description": "charts lib based @visactor/VGrammar",
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "browser": "esm/index.js",
  "types": "esm/index.d.ts",
  "scripts": {
    "build": "bundle --clean",
    "build:ci": "bundle --clean -f umd --minify=false --name ChartSpace",
    "dev": "bundle --clean -f es -w",
    "compile": "tsc --noEmit",
    "start": "ts-node __tests__/runtime/browser/scripts/initVite.ts && vite serve __tests__/runtime/browser",
    "clear": "rm -rf build && rm -rf esm && rm -rf cjs",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check .",
    "eslint": "eslint --debug --fix src/",
    "eslint:check": "eslint .",
    "eslint:err": "eslint . --quiet",
    "postpublish": "npm run clear",
    "test": "jest",
    "test-cov": "jest -w 16 --coverage",
    "test-live": "npm run test-watch __tests__/unit/",
    "test-watch": "DEBUG_MODE=1 jest --watch",
    "ci": "node github-ci.js"
  },
  "files": [
    "esm",
    "cjs",
    "build",
    "chart.d.ts",
    "chart.js",
    "component.d.ts",
    "component.js",
    "core.d.ts",
    "core.js",
    "layout.d.ts",
    "layout.js",
    "mark.d.ts",
    "mark.js",
    "series.d.ts",
    "series.js"
  ],
  "unpkg": "latest",
  "unpkgFiles": [
    "cdn/index.js"
  ],
  "keywords": [
    "charts",
    "visualization",
    "VChart",
    "animation",
    "storytelling",
    "VisActor",
    "graphics",
    "interaction"
  ],
  "homepage": "",
  "bugs": "https://github.com/VisActor/VChart/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/VisActor/VChart.git",
    "directory": "packages/vchart"
  },
  "author": {
    "name": "VisActor",
    "url": "https://VisActor.io/"
  },
  "license": "MIT",
  "devDependencies": {
    "@internal/bundler": "workspace:*",
    "@internal/eslint-config": "workspace:*",
    "@internal/ts-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "@types/jest": "^26.0.0",
    "@types/node": "*",
    "@types/offscreencanvas": "2019.6.4",
    "eslint": "~8.18.0",
    "husky": "7.0.4",
    "jest": "^26.0.0",
    "jest-electron": "^0.1.12",
    "lint-staged": "12.3.7",
    "magic-string": "^0.25.7",
    "prettier": "2.6.1",
    "react-device-detect": "^2.2.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "8.0.2",
    "ts-node": "10.9.0",
    "tslib": "2.3.1",
    "tslint": "5.12.1",
    "typescript": "4.9.5",
    "d3-dsv": "^3.0.1",
    "d3-array": "3.2.3",
    "d3-geo": "^3.0.1",
    "d3-hexbin": "^0.2.2",
    "d3-hierarchy": "^3.1.1",
    "vite": "3.2.6",
    "node-fetch": "2.6.7",
    "form-data": "~4.0.0",
    "canvas": "2.11.2"
  },
  "dependencies": {
    "@visactor/vutils": "~0.13.3",
    "@visactor/vdataset": "~0.13.3",
