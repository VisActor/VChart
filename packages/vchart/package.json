    "@visactor/vgrammar-util": "~0.4.0",
    "@visactor/vrender": "~0.13.0",
    "@visactor/vrender-components": "~0.13.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
