<view tt:if="{{ spec }}" class="container">
  <cs-tooltip class="cs-tooltip"></cs-tooltip>
  <vchart
    class="vchart"
    canvas-id="chart"
    spec="{{ spec }}"
    styles="{{ styles }}"
    events="{{ events }}"
    options="{{ options }}"
    bind:chartinit="onChartInit"
    bind:chartready="onChartReady"
  />
</view>
