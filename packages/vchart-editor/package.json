{
  "name": "@visactor/vchart-editor",
  "version": "0.0.2-beta.7",
  "description": "The VChart editor component.",
  "sideEffects": false,
  "main": "cjs/index.js",
  "module": "es/index.js",
  "types": "es/index.d.ts",
  "files": [
    "cjs",
    "es",
    "dist"
  ],
  "scripts": {
    "compile": "tsc --noEmit",
    "eslint": "eslint --debug --fix src/",
    "build": "bundle --clean",
    "dev": "bundle --clean -f es -w",
    "start": "vite ./__tests__/browser",
    "prepublishOnly": "npm run build",
    "clear": "rm -rf build es cjs dist",
    "prettier": "prettier --write --ignore-unknown .",
    "prettier:check": "prettier --check .",
    "eslint:check": "eslint .",
    "eslint:err": "eslint . --quiet",
    "postpublish": "npm run clear",
    "test": "jest",
    "test-cov": "jest -w 16 --coverage",
    "test-live": "npm run test-watch __tests__/unit",
    "test-watch": "DEBUG_MODE=1 jest --watch"
  },
  "dependencies": {
    "@visactor/vchart": "workspace:1.3.1",
    "@visactor/vutils": "~0.15.11",
    "@visactor/vdataset": "~0.15.11",
    "@visactor/vrender": "~0.14.7"
  },
  "devDependencies": {
    "@internal/bundler": "workspace:*",
    "@internal/eslint-config": "workspace:*",
    "@internal/ts-config": "workspace:*",
    "@rushstack/eslint-patch": "~1.1.4",
    "eslint": "~8.18.0",
    "vite": "3.2.6",
    "typescript": "4.9.5",
    "@types/jest": "^26.0.0",
    "@types/node": "*",
    "@types/offscreencanvas": "2019.6.4",
    "husky": "7.0.4",
    "jest": "^26.0.0",
    "jest-electron": "^0.1.12",
    "lint-staged": "12.3.7",
    "magic-string": "^0.25.7",
    "prettier": "2.6.1",
    "react-device-detect": "^2.2.2",
    "ts-jest": "^26.0.0",
    "ts-loader": "8.0.2",
    "ts-node": "10.9.0",
    "tslib": "2.3.1",
    "tslint": "5.12.1"
  },
  "keywords": [
    "charts",
    "visualization",
    "VChart",
    "editor"
  ],
  "homepage": "https://www.visactor.io/vchart",
  "bugs": "https://github.com/VisActor/VChart/issues",
  "repository": {
    "type": "git",
    "url": "https://github.com/VisActor/VChart.git",
    "directory": "packages/vchart-editor"
  },
  "author": {
    "name": "VisActor",
    "url": "https://www.visactor.io/"
  },
  "license": "MIT",
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/"
  }
}
